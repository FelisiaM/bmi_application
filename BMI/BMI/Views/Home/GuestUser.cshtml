@{
    ViewData["Title"] = "Guest User";
}

@using BMI.Models

@Html.Partial("UserInput")

<h3>@ViewData["Result"]</h3>

<table border="1">
    <tr>
        <th>
            Category
        </th>
        <th>
            Count
        </th>
        <th></th>
    </tr>

    @foreach (var item in (List<ReportModel>) ViewData["PopulationReport"]) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Category)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Count)
            </td>
        </tr>
    }

</table>